<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>AbandonProcessGroup</key>
	<true/>
	<key>Label</key>
	<string>homebrew.mxcl.show-login-window</string>
	<key>ProgramArguments</key>
	<array>
		<string>/System/Library/CoreServices/Menu Extras/User.menu/Contents/Resources/CGSession</string>
		<string>-suspend</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/tmp/homebrew.mxcl.show-login-window.stderr</string>
	<key>StandardOutPath</key>
	<string>/tmp/homebrew.mxcl.show-login-window.stdout</string>
</dict>
</plist>

